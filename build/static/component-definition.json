{
  "groups": [
    {
      "title": "General Components",
      "id": "general",
      "components": [
        {
          "title": "Text",
          "id": "text",
          "plugins": {
            "aem": {
              "page":{
                "resourceType": "wknd/components/text",
                "template": {
                  "text": "Default Text"
                }
              }
            }
          },
          "model": {  
           "id": "text",
            "uri": "",
            "fields": [{
              "component": "text-input",
              "name": "text",
              "value": "Text",
              "label": "Custom Text",
              "valueType": "string"
            }]
          }
        },
        {
          "title": "Title",
          "id": "title",
          "plugins": {
            "aem": {
              "page":{
                "resourceType": "wknd/components/title",
                "template": {
                  "jcr:title": "Default Title"
                }
              }
            }
          },
          "model": {
            "uri": "",
            "id": "title",
            "fields": [{
              "component": "select",
              "name": "type",
              "value": "h1",
              "label": "Type",
              "valueType": "string",
              "options": [
                {"name": "h1","value":"h1"},
                {"name": "h2","value":"h2"},
                {"name": "h3","value":"h3"}
                {"name": "h4","value":"h4"},
                {"name": "h5","value":"h5"},
                {"name": "h6","value":"h6"}
              ]
            }]
          }
        },
        {
          "title": "Image",
          "id": "image",
          "plugins": {
            "aem": {
              "page":{
                "resourceType": "wknd/components/image",
                "template": {
                  "fileReference": "/content/dam/wknd-shared/en/magazine/arctic-surfing/camp-tent.jpg"
                }
              }
            }
          },
          "model": {
            "id": "image",
            "uri": "",
            "fields": [{ 
              "component": "text-input",
              "valueType": "string", 
              "name": "alt", 
              "value": "Default alt", 
              "label": "Alt Text" 
            }]
          }
        },
        {
          "title": "List",
          "plugins": {},
          "model": {
            "uri": "",
            "fields": []
          }
        },
        {
          "title": "Container",
          "id": "container",
          "plugins": {
            "aem": {
              "page":{
                "resourceType": "wknd/components/container"
              }
            }
          },
          "model": {
            "modeltype": "container",
            "uri": "",
            "fields": []
          }
        }
      ]
    },
    {
      "title": "Advanced Components", 
      "id": "advanced",
      "components": [
        {
          "title": "Rich Text",
          "id": "richtext",
          "plugins": {
            "aem": {
              "page":{
                "resourceType": "wknd/components/text",
                "template": {
                  "textIsRich": true,
                  "text": "<p>Default Richtext</p>"
                }
              }
            }
          },
          "model": {
            "uri": "",
            "id": "richtext",
            "fields": [{
              "component": "text-area",
              "name": "text",
              "value": "Text",
              "label": "Custom Richtext",
              "valueType": "string"
            }]
          }
        }
      ]
    }
  ]
}
