{
  "groups": [
    {
      "title": "General Components",
      "id": "general",
      "components": [
        {
          "title": "Text",
          "id": "text",
          "plugins": {
            "aem": {
              "page":{
                "resourceType": "wknd/components/text",
                "template": {
                  "text": "Default Text"
                }
              }
            }
          },
          "model": {  
           "id": "text",
            "uri": "",
            "fields": [{
              "component": "text-input",
              "name": "text",
              "value": "Text",
              "label": "Text",
              "valueType": "string"
            },{
              "component": "boolean",
              "name": "richText",
              "value": false,
              "label": "Toggle RTE",
              "valueType": "boolean"
            }]
          }
        },
        {
          "title": "Image",
          "id": "image",
          "plugins": {
            "aem": {
              "page":{
                "resourceType": "wknd/components/image",
                "template": {
                  "fileReference": "/content/dam/wknd-shared/en/magazine/arctic-surfing/camp-tent.jpg"
                }
              }
            }
          },
          "model": {
            "uri": "",
            "id": "title",
            "fields": [{
              "component": "text-input",
              "name": "text",
              "value": "Title",
              "label": "Title",
              "valueType": "string"
            }]
          }
        },
        {
          "title": "Image",
          "plugins": {},
          "model": {
            "uri": "",
            "fields": []
          }
        },
        {
          "title": "List",
          "plugins": {},
          "model": {
            "uri": "",
            "fields": []
          }
        },
        {
          "title": "Container",
          "id": "container",
          "plugins": {
            "aem": {
              "page":{
                "resourceType": "wknd/components/container"
              }
            }
          },
          "model": {
            "modeltype": "container",
            "uri": "",
            "fields": []
          }
        }
      ]
    },
    {
      "title": "Advanced Components", 
      "id": "advanced",
      "components": [
        {
          "title": "Rich Text",
          "id": "richtext",
          "plugins": {
            "aem": {
              "page":{
                "resourceType": "wknd/components/text",
                "template": {
                  "textIsRich": true,
                  "text": "<p>Default Richtext</p>"
                }
              }
            }
          },
          "model": {
            "uri": "",
            "id": "richtext",
            "fields": [{
              "component": "text-area",
              "name": "text",
              "value": "Text",
              "label": "Text",
              "valueType": "string"
            },{
              "component": "boolean",
              "name": "richText",
              "value": true,
              "label": "Toggle RTE",
              "valueType": "boolean"
            }]
          }
        }
      ]
    }
